{
  "messages": {
    "error": {
      "title": "Error",
      "message": "Gulp reported error: <%= error.message %>!"
    },
    "success": {
      "title": "Success",
      "message": "Gulp tasks completed successfully!"
    }
  },
  "sass": {
    "src": "./sass/**/*.scss",
    "destination": "./assets/stylesheets",
    "sourcemaps": "./",
    "config": {
      "style": "compressed",
      "errLogToConsole": true,
      "includePaths": []
    },
    "autoprefixer": [
      "last 2 versions",
      "> 1%",
      "ie 9",
      "ie 10"
    ]
  },
  "javascript": {
    "file": "app.js",
    "src": "./javascripts/*.js",
    "destination": "./assets/scripts",
    "sourcemaps": "./"
  }
}
